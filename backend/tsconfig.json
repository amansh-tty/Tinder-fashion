{
  "compilerOptions": {
    "target": "es6",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    
  },
  "include": ["src/**/*.ts", "index.ts", "dist/server.ts"],
  "exclude": ["node_modules"]
}
